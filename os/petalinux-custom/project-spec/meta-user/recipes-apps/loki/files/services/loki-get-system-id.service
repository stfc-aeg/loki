[Unit]
SourcePath=/opt/loki-service-scripts/loki-get-system-id.sh
Before=graphical.target
After=loki-bootstrap-emmc.service
Wants=loki-bootstrap-emmc.service
WantedBy=multi-user.target

[Service]
Type=forking
Restart=no
TimeoutSec=5min
IgnoreSIGPIPE=no
KillMode=process
GuessMainPID=no
RemainAfterExit=yes
ExecStart=/opt/loki-service-scripts/loki-get-system-id.sh start
ExecStop=/opt/loki-service-scripts/loki-get-system-id.sh stop
